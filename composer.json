{
    "name": "hackinet/coinbase",
    "description": "Unofficial Coinbase API library",
    "keywords": [ "bitcoin", "coinbase" ],
    "homepage": "https://github.com/Hackinet/coinbase-php-client",
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": { "Coinbase\\Wallet\\": "src/" }
    },
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "^7.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
